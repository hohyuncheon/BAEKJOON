package step3;

import java.util.Scanner;

public class Test2 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		System.out.println("정수를 입력하시오");
		int num1 = sc.nextInt();

		
		
		for (int i = 0; i < num1; i++) {
			System.out.println(i);
			System.out.println(i);
		}
		
	}

}
