package step2;

import java.util.Scanner;

public class Test5 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("시간(0~24시) : ");
		int num1 = sc.nextInt();

		System.out.println("분(0~60분) : ");
		int num2 = sc.nextInt();

		if (num2 < 45) {
			num1--;
			num2 = 60 - (45 - num2);

			if (num1 < 0) {
				num1 = 23;
			}
			System.out.println(num1 + "  " + num2);
		} else {
			System.out.println(num1 + "  " + (num2 - 45));
		}
	}
}
