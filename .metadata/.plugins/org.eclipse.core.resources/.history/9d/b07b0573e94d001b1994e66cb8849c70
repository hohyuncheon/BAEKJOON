		Scanner sc = new Scanner(System.in);
		System.out.println("별숫자");
		int num1 = sc.nextInt();

		for (int i = num1; i >= 0; i--) {
			for (int j = 0; j < i; j++) {
				System.out.print(" ");
				s+="*";
			}System.out.print(s);
