package step2;

public class Test1 {

	public static void main(String[] args) {
		
		int a = 1;
		int b = 2;
		
		if(a>b) {
			System.out.println("<");
		}
		int c = 10;
		int d = 2;
		
		if(c>d) {
			System.out.println(">");
		}
		int e = 10;
		int f = 2;
		
		if(e>f) {
			System.out.println("==");
		}
				

	}

}
